
<config>

  <action>
    <plugin>Plugin_development.MRI.MRI_ACR.PluginQCMRI</plugin>
    <function>MRI_ACR_main</function>
    <default_level>1</default_level>

    <filters>
      <tag name="SeriesDescription">t2 blade tra</tag>
      <tag name="InstanceNumber">7</tag>
    </filters>

    <params>
      <acrtest>ACR_IIU</acrtest>
    </params>
    <limits>
      <result description="Average mean" type="range">
	<acc_onder>0</acc_onder>
	<acc_boven>5</acc_boven>
	<krit_onder>0</krit_onder>
	<krit_boven>8</krit_boven>
      </result>
    </limits>
  </action>


  <action>
    <plugin>Plugin_development.MRI.MRI_ACR.PluginQCMRI</plugin>
    <function>MRI_ACR_main</function>
    <default_level>1</default_level>
    <filters>
      <tag name="SeriesDescription">t2 blade tra</tag>
      <tag name="InstanceNumber">7</tag>
    </filters>
    <params>
      <acrtest>ACR_Ghosting</acrtest>
    </params>
    <limits>
      <result description="Average mean" type="range">
	<acc_onder>0.02</acc_onder>
	<acc_boven></acc_boven>
	<krit_onder>0.025</krit_onder>
	<krit_boven></krit_boven>
      </result>
    </limits>
  </action>



  <action>
    <plugin>Plugin_development.MRI.MRI_ACR.PluginQCMRI</plugin>
    <function>MRI_ACR_main</function>
    <default_level>1</default_level>
    <filters>
      <tag name="SeriesDescription">t2 blade tra</tag>
      <tag name="InstanceNumber">1</tag>
    </filters>
    <params>
      <acrtest>ACR_STA</acrtest>
    </params>
    <limits>
      <result description="Average mean" type="range">
	<acc_onder>4.5</acc_onder>
	<acc_boven>5.5</acc_boven>
	<krit_onder>4.3</krit_onder>
	<krit_boven>5.7</krit_boven>
      </result>
    </limits>
  </action>


